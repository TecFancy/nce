{
  "title": "新概念英语",
  "author": "若非",
  "description": "New Concept English",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "links": {
    "sidebar": {
      "主页": "https://ruofei.site/"
    }
  },
  "styles": {
    "website": "styles/website.css",
    "ebook": "styles/ebook.css",
    "pdf": "styles/pdf.css",
    "mobi": "styles/mobi.css",
    "epub": "styles/epub.css"
  },
  "structure": {
    "readme": "INTRO.md",
    "glossary": "GLOSSARY.md"
  },
  "plugins": [
    "code",
    "search-pro",
    "chapter-fold",
    "popup",
    "advanced-emoji",
    "edit-link",
    "github",
    "sitemap-general",
    "hide-element",
    "-highlight",
    "nce"
  ],
  "pluginsConfig": {
    "sharing": {
      "facebook": false,
      "twitter": false,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "weibo",
        "facebook", 
        "twitter",
        "google", 
        "instapaper",
        "vk"
      ]
    },
    "edit-link": {
      "base": "https://github.com/smpower/supermemo/blob/gitbook",
      "label": "编辑本页"
    },
    "github": {
      "url": "https://github.com/smpower/supermemo.git"
    },
    "sitemap-general": {
      "prefix": "https://supermemo.ruofei.site/"
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    }
  }
}
  